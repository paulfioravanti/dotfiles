snippet div "<div>" w
<div${1:${2: id="${3:id}"}${4: class="${5:class}"}}>
  ${0:${VISUAL}}
</div>
endsnippet

snippet "h([1-6])" "HTML heading" r
<h`!p snip.rv = match.group(1)`>${0:${VISUAL}}</h`!p snip.rv = match.group(1)`>
endsnippet

snippet p "<p>" w
<p${1:${2: id="${3:id}"}${4: class="${5:class}"}}>
  ${0:${VISUAL}}
</p>
endsnippet

snippet strong "<strong>" w
<strong>${0:${VISUAL}}</strong>
endsnippet
